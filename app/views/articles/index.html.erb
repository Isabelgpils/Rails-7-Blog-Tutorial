<div class="home-page">
  <div class="image-container">
    <h1 class= "heading">Midwest Wildflowers</h1>
  </div>
  <div class= "articles-container">
    <h2>September Articles</h2>
    <ul>
      <% @articles.each do |article| %>
        <li class= "article-list">
          <h4 class="nudge-down-15 article-list"> <%= link_to article.title, article %></h4>
        </li>
      <% end %>
    </ul>
    <button class="button"><%= link_to "New Article", new_article_path %></button>
  </div>
</div>
